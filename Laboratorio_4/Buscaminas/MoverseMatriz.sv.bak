module MoverseMatriz;
  reg [7:0] matriz [0:7][0:7];

  initial begin
    // Inicializar la matriz con algunos valores
    for (int i = 0; i < 8; i++)
      for (int j = 0; j < 8; j++)
        matriz[i][j] = i + j;

    // Moverse a travÃ©s de la matriz
    for (int i = 0; i < 8; i++) begin
      for (int j = 0; j < 8; j++) begin
        $display("matriz[%0d][%0d] = %0d", i, j, matriz[i][j]);
      end
    end
  end
endmodule